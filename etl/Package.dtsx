                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    s.comuna_id,
    v.cliente_id,
    dv.producto_id,
    v.vendedor_id AS empleado_id,
    v.sucursal_id,
    v.fecha,
    SUM(dv.cantidad) AS cantidad_venta,
    SUM(dv.precio_unitario) AS monto_venta,
    AVG(dv.precio_unitario / dv.cantidad) AS promedio_venta
FROM detalle_ventas dv
INNER JOIN productos pr ON dv.producto_id = pr.producto_id
INNER JOIN ventas v ON dv.venta_id = v.venta_id
INNER JOIN clientes cli ON v.cliente_id = cli.cliente_id
INNER JOIN sucursales s ON v.sucursal_id = s.sucursal_id
GROUP BY 
    s.comuna_id,
    v.cliente_id,
    dv.producto_id,
    v.vendedor_id,
    v.sucursal_id,
    v.fecha</property>
